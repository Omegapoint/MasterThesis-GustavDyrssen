{
  "name": "xtreme-react-admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://wrappixel.com/demos/free-admin-templates/xtreme-reactadmin-lite/",
  "dependencies": {
    "@date-io/date-fns": "^1.1.0",
    "@material-ui/core": "^3.9.2",
    "@types/date-fns": "^2.6.0",
    "bootstrap": "4.1.3",
    "chart.js": "2.7.2",
    "date-fns": "^2.0.0-alpha.27",
    "heatmap.js": "^2.0.5",
    "history": "4.7.2",
    "material-ui-pickers": "^2.2.1",
    "node-sass": "4.6.1",
    "node-sass-chokidar": "1.3.3",
    "npm-run-all": "4.1.3",
    "react": "16.4.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "16.4.2",
    "react-hotjar": "^2.0.0",
    "react-leaflet-heatmap-layer": "^2.0.0",
    "react-native-ux-cam": "^5.0.1",
    "react-perfect-scrollbar": "1.2.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.8",
    "react-switch": "^4.1.0",
    "reactstrap": "6.4.0",
    "semantic-ui-react": "^0.86.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
